<!-- Extendendo as partes repetidas para o layout.njk -->
{% extends "layout.njk" %}

<!-- Conteúdo do bloco head irá substituir o head no layout.njk -->
{% block head %}
  <title>Cursos - Rocketseat</title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
{% endblock %}

<!-- Conteúdo do bloco content irá substituir o conteúdo no layout.njk -->
{% block content %}
    <div id="course">
      <h1>Descrição do {{data.alt}}</h1>
    </div>
    <div class="page">
      <iframe src="https://rocketseat.com.br/{{id}}" frameborder="0"></iframe>
    </div>

{% endblock %}