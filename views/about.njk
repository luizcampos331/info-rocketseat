<!-- Extendendo as partes repetidas para o layout.njk -->
{% extends "layout.njk" %}

<!-- Conteúdo do bloco head irá substituir o head no layout.njk -->
{% block head %}
  <title>Sobre - Rocketseat</title>
{% endblock %}

<!-- Conteúdo do bloco content irá substituir o conteúdo no layout.njk -->
{% block content %}
    <div id="about">
        <img src="https://avatars0.githubusercontent.com/u/28929274?s=200&v=4" alt="Rocketseat">
        <h1>Rocketseat</h1>
        <h2>Plataforma de educação em tecnologia</h2>
        <p>Principais tecnologias:</p>
        <ul>
          <li>HTML</li>
          <li>CSS</li>
          <li>JavaScript</li>
          <li>NodeJS</li>
          <li>ReactJS</li>
          <li>React Native</li>
        </ul>
    </div>

    <div id="links-footer" class="links">
      <a href="https://github.com/Rocketseat" target="_blank">GitHub</a>
      <a href="https://www.facebook.com/rocketseat" target="_blank">Instagram</a>
      <a href="https://www.instagram.com/rocketseat_oficial/" target="_blank">Facebook</a>
    </div>
{% endblock %}